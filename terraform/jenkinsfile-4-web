pipeline  {
    agent any 
    
    stages {
        
        stage (' Deploy Packages   SSH') {
            
            steps {
                sshagent (credentials: ['AWS-SSH-KEY']) {
                    sh """
                        ssh -o StrictHostKeyChecking=no  ec2-user@172.31.43.107 ' cd ~; rm -rf fruits-veg_market ;git clone https://github.com/techbleat/fruits-veg_market.git; cd fruits-veg_market; sudo cp web/index.html  /usr/share/nginx/html/ ' 
                    """
                }

            }
        }
    }
}
